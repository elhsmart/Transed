{
    "name": "Transed",
    "description": "Simple text editor for translators.",
    "version": "0.1",
    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "permissions": [
        {"fileSystem": ["write"]},
        "http://mymemory.translated.net/api/"
    ],
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
    "icons": { "16": "images/transed-16.png", "128": "images/transed-128.png" }
}